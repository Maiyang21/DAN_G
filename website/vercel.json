{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://dan-g-platform.vercel.app",
    "NEXTAUTH_SECRET": "@nextauth_secret",
    "NEXT_PUBLIC_API_URL": "@api_url"
  },
  "functions": {
    "frontend/pages/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
